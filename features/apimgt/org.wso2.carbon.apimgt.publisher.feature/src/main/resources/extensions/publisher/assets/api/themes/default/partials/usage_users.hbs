<div class="row-fluid" id="usageSummary" style="display:none;">
	<div class="span6">
		<h3>{{t locale.usageSummary}}</h3>
		<table class="table table-bordered">
			<tbody id="usageTable">
		</table>
	</div>
</div>

<div class="row-fluid">
	<div class="span12">
		<h3>{{t locale.activeSubscriptions}}</h3>
		<table id="subscribers" class="table table-striped table-bordered" cellspacing="0" width="100%">
			<thead>
			<tr>
				<th class="span4">{{t locale.name}}</th>
				<th>{{t locale.dateOfUserRegistration}}</th>
			</tr>
			</thead>
			<tbody id="userList">
			{{#unless subscribers}}
				<tr><td colspan="3">{{t locale.noActiveSubscribersMsg}}</td></tr>
			{{else}}
				{{#each subscribers}}
					<tr>
						<td><i class="fa fa-user">&nbsp;</i>{{this.name}}</td>
						<td class="dateFull">{{this.subscribedDate}}</td>
					</tr>
				{{/each}}
			{{/unless}}
			</tbody>
		</table>
	</div>
</div>

<!-- Row -->

<h3>{{t locale.usageByCurrentSubscribers}} (v-{{apiIdentifier.version}})</h3>

<div id="usageByCurrentSubscribersSpinner" class="center-wrapper">
	<i id="icon-spinner" class="fa fa-spinner fa-pulse fa-4x"></i>
</div>

<div class="row">
	<div id="usageByCurrentSubscribersNoData"></div>
	<div class="col-sm-6">
		<div id="userVersionChart"></div>
	</div>
	<div class="col-sm-6">
		<br />
		<br />
		<table class="display table table-striped table-bordered" cellspacing="0" width="100%" id="userVersionTable" style="display:none">
			<thead>
			<tr>
				<th>{{t locale.subscriber}}</th>
				<th>{{t locale.noOfAPICalls}}</th>
			</tr>
			</thead>
		</table>
	</div>
</div>



<h3>{{t locale.usageByCurrentSubscribersAcrossAll}}</h3>

<div id="usageByCurrentSubscribersAcrossAllSpinner" class="center-wrapper">
	<i id="icon-spinner" class="fa fa-spinner fa-pulse fa-4x"></i>
</div>

<div class="row">
	<div id="usageByCurrentSubscribersAcrossAllNoData"></div>
	<div class="col-sm-6">
		<div id="userChart"></div>
	</div>
	<div class="col-sm-6">
		<br />
		<br />
		<table class="display table table-striped table-bordered" cellspacing="0" width="100%" id="userTable" style="display:none">
			<thead>
			<tr>
				<th>{{t locale.subscriber}}</th>
				<th>{{t locale.noOfAPICalls}}</th>
			</tr>
			</thead>
		</table>
	</div>
</div>
